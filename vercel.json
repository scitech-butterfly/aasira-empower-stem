{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "aasira.site"
        }
      ],
      "destination": "https://www.aasira.site",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
